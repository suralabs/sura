    <input type="text" id="phone" class="inpst" style="width:180px;margin-bottom:5px"/>
    <div class="texta" style="width:80px">E-Mail:</div>
    <input type="text" id="email" class="inpst" style="width:180px;margin-bottom:5px"/>
</div>